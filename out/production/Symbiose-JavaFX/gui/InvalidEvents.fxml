<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="500.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.InvalidEvents">
    <children>
        <TableView fx:id="tvInvalidEvents" layoutX="160.0" layoutY="87.0" onMouseClicked="#selectEvent" prefHeight="361.0" prefWidth="581.0">
            <columns>
                <TableColumn fx:id="colName" prefWidth="75.0" text="Name" />
                <TableColumn fx:id="colParticipants" prefWidth="75.0" text="Participants" />
                <TableColumn fx:id="colRemaining" prefWidth="75.0" text="Remaining" />
                <TableColumn fx:id="colType" prefWidth="75.0" text="Type" />
                <TableColumn fx:id="colDate" prefWidth="75.0" text="Date" />
                <TableColumn fx:id="colState" prefWidth="75.0" text="State" />
            </columns>
        </TableView>
        <TextField fx:id="tfinvalidSearch" layoutX="376.0" layoutY="46.0" promptText="Search" />
            <Button fx:id="btnValid" disable="true" layoutX="400.0" layoutY="448.0" mnemonicParsing="false" onAction="#validateEvent" prefWidth="100.0" text="Validate" />
        <TextField fx:id="tfinvalidID" layoutX="697.0" layoutY="58.0" prefHeight="25.0" prefWidth="44.0" promptText="ID" />
      <Text layoutX="615.0" layoutY="75.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Event selected" />
      <Text layoutX="364.0" layoutY="26.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Validate events">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Text>
    </children>
</AnchorPane>
